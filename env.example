# 데이터베이스 모드 설정 (supabase | cms | offline)
VITE_DATABASE_MODE=cms

# Supabase 설정 (DATABASE_MODE=supabase일 때 사용)
VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# CMS 서버 설정 (DATABASE_MODE=cms일 때 사용)
VITE_CMS_API_URL=http://your-cms-server.com:3000/api
VITE_CMS_API_KEY=your_cms_api_key_here
VITE_CMS_AUTH_TOKEN=your_cms_auth_token

# 키오스크 식별 설정
VITE_KIOSK_ID=cvm_kiosk_001
VITE_STORE_ID=store_001
VITE_DEFAULT_STORE_ID=store_default

# 애플리케이션 설정
VITE_APP_NAME=CVM Kiosk
VITE_APP_VERSION=2.0.0
VITE_APP_ENV=production

# 키오스크 설정
VITE_KIOSK_MODE=true
VITE_AUTO_START=true
VITE_FULLSCREEN=true

# 결제 설정
VITE_PAYMENT_CARD_ENABLED=true
VITE_PAYMENT_QR_ENABLED=true
VITE_PAYMENT_CASH_ENABLED=true

# 디스플레이 설정
VITE_DISPLAY_WIDTH=1920
VITE_DISPLAY_HEIGHT=1080

# 로그 설정
VITE_LOG_LEVEL=info
VITE_LOG_UPLOAD_ENABLED=true
VITE_LOG_UPLOAD_INTERVAL=300000

# 보안 설정
VITE_ADMIN_PASSWORD=admin123
VITE_ENCRYPTION_KEY=your_encryption_key_here

# 디버그 설정
VITE_DEBUG_MODE=false
VITE_DEV_TOOLS_ENABLED=false 